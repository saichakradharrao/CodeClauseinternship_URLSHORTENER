   "source": [
    "import pyshorteners\n",
    "\n",
    "long_url = input(\"enter the URL\")\n",
    "\n",
    "def short_url(url):\n",
    "\n",
    "    type_tiny = pyshorteners.Shortener()\n",
    "\n",
    "    short_url = type_tiny.tinyurl.short(url)\n",
    "\n",
    "    return short_url\n",
    "\n",
    "shortened_url=short_url(long_url)\n",
    "\n",
    "\n",
    "print(\"The Shortened URL is: \"+shortened_url)"
   ]
  }
